#include <stdio.h>

int main(){
    int num[4], maks = -1;
    for (int i = 0; i < 4; i++)
    {
        scanf("%d", &num[i]);
        if(maks < num[i]){
            maks = num[i];
        }
    }
    for (int i = 0; i < 4; i++)
    {
        if(maks > num[i]){
            printf("%d ", maks-num[i]);
        }
    }
    
    
    return 0;
}
