#include <stdio.h>

int main(){
    int a, b, c, n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d %d %d", &a, &b, &c);
        if(c>=0){
            if(c >= a && c >= b){
                printf("+");
            }else if(a>=b){
                printf("-");
            }
        }else{
            printf("-");
        }
        printf("\n");
    }
    return 0;
}
