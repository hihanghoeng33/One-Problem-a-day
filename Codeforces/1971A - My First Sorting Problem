#include <stdio.h>

int main(){
    int n;
    int a, b;
    scanf("%d", &n);
    getchar();
    for (int i = 0; i < n; i++)
    {
        scanf("%d %d", &a, &b);
        getchar();
        if(a > b){
            printf("%d %d", b, a);
        }else{
            printf("%d %d", a, b);
        }
        printf("\n");
    }
    return 0;
}
