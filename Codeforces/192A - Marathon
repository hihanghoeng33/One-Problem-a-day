#include <stdio.h>

int main(){
    int arr[4];
    int n, Timur = arr[0], count = 0;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        count = 0;
        for (int j = 0; j < 4; j++)
        {
            scanf("%d", &arr[j]);
            Timur = arr[0];
            if (arr[j]>Timur)
            {
                count++;
            }
            
        }
        printf("%d\n", count);
    }
    
    return 0;

}
