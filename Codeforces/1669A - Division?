#include <stdio.h>

int main(){
    int n, num;
    scanf("%d", &n);
    for(int i=0;i<n;i++){
        scanf("%d", &num);
        if(num<=1399){
            printf("Division 4");
        }else if(num<=1599){
            printf("Division 3");
        }else if(num<=1899){
            printf("Division 2");
        }else{
            printf("Division 1");
        }
        printf("\n");
    }
    return 0;
}
