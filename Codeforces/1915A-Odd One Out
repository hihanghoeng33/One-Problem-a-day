#include <stdio.h>

int main()
{
    int n, a, b, c;
    scanf("%d", &n);
    for(int i=0;i<n;i++){
        scanf("%d %d %d", &a, &b, &c);
        if(a==b){
            printf("%d", c);
        }else if(a==c){
            printf("%d", b);
        }else if(b==c){
            printf("%d", a);
        }
        printf("\n");
    }

    return 0;
}
